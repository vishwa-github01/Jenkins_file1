pipeline {
  agent any 
   stages {
      stage ("build") {
       steps {
        echo "This is the Build Stage"
       }
    }
     
    stage ("test") {
       steps {
        echo "This is the test Stage"
       }
    }
    
    stage ("Deploy") {
       steps {
        echo "This is the Deploy Stage"
       }
    }
  }  
}
